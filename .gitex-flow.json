{
    "gitFlowConfig": {
        "masterBranch": "main",
        "developBranch": "next",
        "featureBranchPrefix": "feature",
        "bugfixBranchPrefix": "bugfix",
        "releaseBranchPrefix": "release",
        "hotfixBranchPrefix": "hotfix",
        "supportBranchPrefix": "support",
        "versionTagPrefix": "v"
    },
    "projectConfig": {
        "projectPath": "./",
        "autoStash": true,
        "changelogFileName": "CHANGELOG.md",
        "storeLatestChangelog": false,
        "conventionalChangelogPresent": "angular",
        "changelog": {
            "type": "ConventionalChangelog",
            "changelogFileName": "CHANGELOG.md",
            "storeLatestChangelog": false,
            "conventionalChangelogPresent": "angular"
        },
        "conventionalCommit": {
            "referenceActions": [
                "close",
                "closes",
                "closed",
                "fix",
                "fixes",
                "fixed",
                "resolve",
                "resolves",
                "resolved",
                "refs",
                "references"
            ],
            "noteKeywords": [
                "BREAKING CHANGE",
                "SECURITY",
                "REMOVED"
            ]
        },
        "versionFile": "package.json",
        "bumpVersionFiles": [
            "package.json",
            "package-lock.json"
        ]
    },
    "log4jsConfig": {
        "appenders": {
            "console": {
                "type": "console"
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "console"
                ],
                "level": "info"
            }
        }
    }
}